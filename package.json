{
  "name": "helicopter",
  "version": "1.0.0",
  "description": "Simple crud api using mongo express docker",
  "main": "index.js",
  "scripts": {
    "dev": "docker-compose -f docker/docker-compose.yml up",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "docker-compose -f docker/docker-compose.yml build",
    "local": "nodemon --exec babel-node src/index.js",
    "lint": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.js": [
      "prettier --write",
      "npm run lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "grudTry"
  },
  "author": "EkaterinaI",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "install": "^0.13.0",
    "mongodb": "^3.3.3",
    "mongoose": "^5.7.7",
    "mongoose-autopopulate": "^0.9.1",
    "npm": "^6.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.3",
    "nodemon": "^1.19.4",
    "prettier": "1.19.1"
  }
}
